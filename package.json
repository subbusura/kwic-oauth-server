{
  "name": "oauth-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "migrate": "node ./scripts/run_migrations.js"
  },
  "dependencies": {
    "@authenio/samlify-node-xmllint": "^2.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^1.82.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-random-string": "^4.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jose": "^4.15.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.0",
    "oidc-provider": "^7.0.0",
    "samlify": "^2.8.9",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.50.0",
    "husky": "^8.0.0",
    "jest": "^29.6.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.8.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --fix"
  }
}
